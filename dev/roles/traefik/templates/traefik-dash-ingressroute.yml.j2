apiVersion: traefik.io/v1alpha1
kind: IngressRoute
metadata:
  name: traefik-dash-ingressroute
  namespace: traefik
spec:
  entryPoints:
    - websecure
  routes:
    - match: Host(`{{ traefik_dash_fqdn }}`)
      kind: Rule
      services:
        - name: traefik
          port: 8080
  tls:
    secretName: traefik-certificate-secret