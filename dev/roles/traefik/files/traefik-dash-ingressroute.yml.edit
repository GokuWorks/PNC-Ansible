apiVersion: traefik.io/v1alpha1
kind: IngressRoute
metadata:
  name: traefik-dash-ingressroute
  namespace: traefik
spec:
  entryPoints:
    - websecure
  routes:
    - match: Host(`traefik-dash.{FQDN}`)
      kind: Rule
      services:
        - name: traefik
          port: 443
  tls:
    secretName: traefik-certificate-secret