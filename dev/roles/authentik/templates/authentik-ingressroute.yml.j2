apiVersion: traefik.io/v1alpha1
kind: IngressRoute
metadata:
  name: authentik-ingressroute
  namespace: authentik
spec:
  entryPoints:
    - websecure
  routes:
    - match: Host(`{{ authentik_fqdn }}`)
      kind: Rule
      services:
        - name: authentik
          port: 9443
  tls:
    secretName: authentik-certificate-secret